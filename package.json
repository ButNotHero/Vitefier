{
  "name": "client",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=14",
    "yarn": "^1.22.17"
  },
  "scripts": {
    "dev": "yarn run clean:generated && vite-ssr --port 3333 --host",
    "dev:spa": "yarn run clean:generated && vite --port 3333 --host",
    "start": "yarn run clean && yarn run build && yarn run server:production",
    "build": "cross-env NODE_ENV=production vite-ssr build",
    "server:production": "yarn --cwd ./_app/server/ run production",
    "clean": "rimraf _app/dist",
    "clean:generated": "rimraf src/auto-imports.d.ts && rimraf src/components.d.ts",
    "fix": "yarn fix:eslint && yarn fix:prettier && yarn fix:stylelint",
    "fix:eslint": "eslint **/*.{js,ts,vue} --fix",
    "fix:prettier": "prettier --write **/*.{vue,js,ts,css,sass,scss}",
    "fix:stylelint": "stylelint --fix **/*.{vue,css,sass,scss} --ignore-path"
  },
  "dependencies": {
    "@nuxt/devalue": "^2.0.0",
    "@vueuse/core": "^7.4.0",
    "@vueuse/head": "^0.7.4",
    "axios": "^0.24.0",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.8",
    "vite-ssr": "^0.14.3",
    "vite-ssr-middleware": "^0.2.1",
    "vue": "^3.2.26",
    "vue-demi": "^0.12.1",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@prettier/plugin-pug": "^1.19.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^17.0.2",
    "@types/node-fetch": "^3.0.3",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@windicss/plugin-scrollbar": "^1.2.3",
    "critters": "^0.0.15",
    "cross-env": "^7.0.3",
    "cypress": "^9.1.1",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "pug": "^3.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.45.1",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.5.3",
    "unplugin-vue-components": "^0.17.9",
    "vite": "^2.7.4",
    "vite-plugin-inspect": "^0.3.11",
    "vite-plugin-pages": "^0.19.6",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vite-plugin-windicss": "^1.5.4",
    "vite-ssg": "^0.17.2",
    "vue-tsc": "^0.29.8",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "commit-msg": "commitlint -e -V",
    "pre-commit": "lint-staged",
    "pre-push": "lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx}": "eslint --fix",
    "*.{vue,js,ts,jsx,tsx,css,sass,scss}": "prettier --write",
    "*.{vue,css,sass,scss}": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  }
}
